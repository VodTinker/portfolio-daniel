# Cloudflare Pages - Custom Headers

# SVG files in github-assets folder (correct spelling)
/github-assets/*.svg
  Content-Type: image/svg+xml
  Access-Control-Allow-Origin: *
  Cache-Control: public, max-age=31536000

# SVG files in github-assests folder (typo version - for backwards compatibility)
/github-assests/*.svg
  Content-Type: image/svg+xml
  Access-Control-Allow-Origin: *
  Cache-Control: public, max-age=31536000

  # Matrix well-known
/.well-known/matrix/*
  Access-Control-Allow-Origin: *
  Content-Type: application/json
